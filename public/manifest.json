{
    "manifest_version": 3,
    "name": "chrome extension-vanilla-ts-starter",
    "version": "1.0",
    "description": "",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "service-worker/main.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content-script/main.js"
            ]
        }
    ],
    "permissions": [
        "scripting",
        "tabs",
        "activeTab"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "icons": {
        "16": "images/16x16.png",
        "32": "images/32x32.png"
    }
}